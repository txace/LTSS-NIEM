  <xs:element name="AssociationDateRange" nillable="true" type="nc:DateRangeType">
    <xs:annotation>
      <xs:documentation>A date range in which an association occurs.
			</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AssociationDescriptionText" nillable="true" type="nc:TextType">
    <xs:annotation>
      <xs:documentation>A description of an association.
			</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element abstract="true" name="Contact">
    <xs:annotation>
      <xs:documentation>A data concept for a person or organization to
				contact.</xs:documentation>
    </xs:annotation>
  </xs:element>
